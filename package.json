{
  "name": "lambda-base",
  "version": "1.0.0",
  "description": "Base folder structure for Lambda projects",
  "main": "lambda.js",
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.2.16",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "chai": "^4.3.4",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-standard": "^5.0.0",
    "prettier": "^2.2.1",
    "mocha": "^8.3.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "build": "npm run lint-and-fix && tsc",
    "test": "mocha --require ts-node/register test/**/*.ts",
    "package": "npm run build && copy .\\package.json .\\dist\\src\\functions",
    "sam-build": "npm run package && sam build",
    "delete-stack": "aws cloudformation delete-stack --stack-name lambda-base --region eu-west-2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andybalham/LambdaBase.git"
  },
  "keywords": [
    "Lambda"
